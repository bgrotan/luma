<connections>
    <connection>
        <sender>addTemplateButton</sender>
        <signal>clicked()</signal>
        <receiver>TemplateFormDesign</receiver>
        <slot>add_template()</slot>
    </connection>
    <connection>
        <sender>delTemplateButton</sender>
        <signal>clicked()</signal>
        <receiver>TemplateFormDesign</receiver>
        <slot>delete_template()</slot>
    </connection>
    <connection>
        <sender>templateView</sender>
        <signal>selectionChanged()</signal>
        <receiver>TemplateFormDesign</receiver>
        <slot>update_view()</slot>
    </connection>
    <connection>
        <sender>classButton</sender>
        <signal>clicked()</signal>
        <receiver>TemplateFormDesign</receiver>
        <slot>edit_classes()</slot>
    </connection>
    <connection>
        <sender>saveButton</sender>
        <signal>clicked()</signal>
        <receiver>TemplateFormDesign</receiver>
        <slot>save_template()</slot>
    </connection>
</connections>
<slots>
    <slot>add_template()</slot>
    <slot>delete_template()</slot>
    <slot>update_view()</slot>
    <slot>edit_classes()</slot>
    <slot>edit_attributes()</slot>
    <slot>save_template()</slot>
</slots>
